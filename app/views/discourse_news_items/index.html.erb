<p id="notice"><%= notice %></p>

<h1>Discourse News Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @discourse_news_items.each do |discourse_news_item| %>
      <tr>
        <td><%= link_to 'Show', discourse_news_item %></td>
        <td><%= link_to 'Edit', edit_discourse_news_item_path(discourse_news_item) %></td>
        <td><%= link_to 'Destroy', discourse_news_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Discourse News Item', new_discourse_news_item_path %>
