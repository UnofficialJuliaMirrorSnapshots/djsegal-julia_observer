<%= form_for(info) do |f| %>
  <% if info.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(info.errors.count, "error") %> prohibited this info from being saved:</h2>

      <ul>
      <% info.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :repos %>
    <%= f.number_field :repos %>
  </div>

  <div class="field">
    <%= f.label :followers %>
    <%= f.number_field :followers %>
  </div>

  <div class="field">
    <%= f.label :following %>
    <%= f.number_field :following %>
  </div>

  <div class="field">
    <%= f.label :created %>
    <%= f.datetime_select :created %>
  </div>

  <div class="field">
    <%= f.label :owner_id %>
    <%= f.text_field :owner_id %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
